   "source": [
    "def number_of_ways(n:int, m:int)->int:\n",
    "    y = 1\n",
    "    x = 1\n",
    "    z = 1\n",
    "    for i in range(1,n+m-1):\n",
    "        y *= i\n",
    "    for j in range(1,n):\n",
    "        x *= j\n",
    "    for k in range(1,m):\n",
    "        z *= k\n",
    "    return int(y/(x*z))"
   ]
